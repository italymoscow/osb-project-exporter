  <xsl:template name="toDate">
    <xsl:param name="YYYYMMDD"/>
    <xsl:param name="dateTime"/>
    <xsl:choose>
      <xsl:when test="$YYYYMMDD = '0'">0000-00-00</xsl:when>
      <xsl:when test="$YYYYMMDD != ''">
        <xsl:value-of select="concat(substring($YYYYMMDD, 1, 4), '-', substring($YYYYMMDD, 5, 2), '-', substring($YYYYMMDD, 7, 2))"/>
      </xsl:when>
      <xsl:when test="$dateTime != ''">
        <xsl:value-of select="substring($dateTime, 1, 10)"/>
      </xsl:when>
    </xsl:choose>
  </xsl:template>
  <xsl:template name="toHHmm">
    <xsl:param name="dateTime"/>
    <xsl:value-of select="concat(substring($dateTime, 12, 2), substring($dateTime, 15, 2))"/>
  </xsl:template>
  <xsl:template name="shiftDate">
    <xsl:param name="date"/>
    <xsl:param name="duration"/>
    <xsl:call-template name="toDate">
      <xsl:with-param name="dateTime"
                      select="xp20:add-dayTimeDuration-to-dateTime(concat($date, 'T00:00:00') , $duration)"/>
    </xsl:call-template>
  </xsl:template>
